{
    "name": "Youtube View Boot",
    "description": "Extension to increase views on YouTube video just by one tap!!!",
    "version": "0.1.0",
    "author": "Adnan Malik | dCodes",
    "icons": {
        "48": "icon-48.png"
    },
    "background": {
        "service_worker": "background.js",
        "matches":"<all_urls>"
    },
    "action": {
        "default_title": "Created by Adnan Malik",
        "default_popup": "popup.html",
        "default_icon": {
            "64":"icon-48.png",
            "48": "icon-48.png",
            "32": "icon-48.png",
            "16": "icon-48.png"
        }
    },
    "manifest_version": 3,
    "permissions": [
        "tabs",
        "activeTab",
        "storage",
        "bookmarks",
        "scripting",
        "notifications",
        "alarms"
    ],
    "host_permissions": [
        "http://*/",
        "https://*/"
      ]
}